/dts-v1/;

#include "rk3399-bainarm-jd.dtsi"
#include "rk3399-android.dtsi"
#include "rk3399-vop-clk-set.dtsi"

/ {
	model = "Rockchip RK3399 bainarm jd(Android)";
	compatible = "rockchip,android", "rockchip,rk3399-bainarm-android-jd", "rockchip,rk3399";

	test-power {
		status = "okay";
	};

	rt5651-sound {
		status = "okay";
	};
};

&hdmi {
	status = "okay";
};

&rt5651 {
	status = "okay";
};

&isp0 {
	status = "okay";
};

&isp1 {
	status = "okay";
};

&isp0_mmu {
	status = "okay";
};

&isp1_mmu {
	status = "okay";
};

&vopb {
	assigned-clocks = <&cru DCLK_VOP0_DIV>;
	assigned-clock-parents = <&cru PLL_CPLL>;
};

&vopl {
	assigned-clocks = <&cru DCLK_VOP1_DIV>;
	assigned-clock-parents = <&cru PLL_VPLL>;
};
