&spi2 {
	status = "okay";
	max-freq = <48000000>;  
	spidev@00 {
		compatible = "linux,spidev";
		reg = <0x00>;
		spi-max-frequency = <48000000>;
	};
};

&io_domains {
	status = "okay";

	//bt656-supply = <&vcc1v8_dvp>;
